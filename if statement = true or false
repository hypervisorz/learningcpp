#include <iostream>
using namespace std;

int main()
{
	int num;
	cout << "whats 10+10" << endl;
	cin >> num;

	if (num ==20) {
		cout << "correct" << endl;
	}

	if (num !=20) {
		cout << "wrong" << endl;
	}
}

 // using if (num ==x) means it has to be that number or its false
 (num !=x) less than and not true.
